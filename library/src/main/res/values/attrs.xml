<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CarouselView">
        <attr name="cv_autoStart" format="boolean"/>
        <attr name="cv_interval" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PagerView">
        <attr name="pv_orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="CollapseTextView">
        <attr name="collapseText" format="string"/>
        <attr name="collapseTextSize" format="integer"/>
        <attr name="collapseTextColor" format="color"/>
        <attr name="collapseIcon" format="reference"/>
        <attr name="collapsed" format="boolean"/>
        <attr name="availableCollapse" format="boolean"/>
        <attr name="contentMaxLine" format="integer"/>
    </declare-styleable>
</resources>